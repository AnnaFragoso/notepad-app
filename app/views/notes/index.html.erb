  <body>
    <h1>Bloco de notas</h1>
    <%= link_to "Nova nota", new_note_path %>
    <br>
    <br>
    <div class="card">
      <tbody>
        <% @notes.each do |note| %>
          <tr>
          <td scope="row"><%= note.title %></td>
          <br>
          <td scope="row"><%= note.note %></td>
          <br>
          <td scope="row"><%= note.priority %></td>
          <br>
          <td><%= link_to "Show", note %></td>
          <td><%= link_to "Edit", edit_note_path(note) %></td>
          <td><%= button_to "Delete", note, method: :delete %></td>
          <br>
          <br>
          </tr>
        <% end %>
      </tbody>
    </div>
    <!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script> -->
